{
  "name": "meta-pkg",
  "version": "0.2.1",
  "description": "A way to install software natively in a cross-platform manner.",
  "keywords": [
    "install",
    "installer",
    "package",
    "packagekit",
    "brew",
    "homebrew",
    "linuxbrew"
  ],
  "main": "out/source/index.js",
  "typings": "out/typings/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "compile": "node ./node_modules/.bin/tsc -t ES2015 --moduleResolution node -watch -p ./",
    "preversion": "node ./node_modules/.bin/tsc -t ES2015 --moduleResolution node -d ./source/index.ts --outdir out/typings"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/LaurentTreguier/meta-pkg"
  },
  "author": "LaurentTreguier <laurent@treguier.org>",
  "license": "GPL-3.0",
  "dependencies": {
    "request": "^2.79.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "@types/request": "^0.0.36",
    "typescript": "^2.1.4"
  }
}
