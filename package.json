{
  "name": "meta-pkg",
  "version": "0.7.6",
  "description": "A way to install software natively in a cross-platform manner.",
  "keywords": [
    "install",
    "installer",
    "package",
    "packagekit",
    "brew",
    "homebrew",
    "linuxbrew"
  ],
  "main": "out/source/index.js",
  "typings": "out/typings/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "compile": "tsc -t ES2015 --moduleResolution node -watch -p .",
    "preversion": "tsc -t ES2015 --moduleResolution node -d ./source/index.ts --outdir out/typings"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/LaurentTreguier/meta-pkg"
  },
  "author": "LaurentTreguier <laurent@treguier.org>",
  "license": "GPL-3.0",
  "dependencies": {
    "decompress": "^4.2.0",
    "fs-extra": "^8.0.1",
    "request": "^2.88.0",
    "sax": "^1.2.4",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^10.x.x",
    "@types/request": "^2.48.1",
    "@types/sax": "^1.2.0",
    "@types/tmp": "^0.1.0",
    "typescript": "^3.5.1"
  }
}
