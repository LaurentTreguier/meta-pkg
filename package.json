{
  "name": "meta-pkg",
  "version": "0.6.4",
  "description": "A way to install software natively in a cross-platform manner.",
  "keywords": [
    "install",
    "installer",
    "package",
    "packagekit",
    "brew",
    "homebrew",
    "linuxbrew"
  ],
  "main": "out/source/index.js",
  "typings": "out/typings/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "compile": "./node_modules/.bin/tsc -t ES2015 --moduleResolution node -watch -p .",
    "preversion": "./node_modules/.bin/tsc -t ES2015 --moduleResolution node -d ./source/index.ts --outdir out/typings"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/LaurentTreguier/meta-pkg"
  },
  "author": "LaurentTreguier <laurent@treguier.org>",
  "license": "GPL-3.0",
  "dependencies": {
    "decompress": "^4.0.0",
    "fs-extra": "^2.0.0",
    "request": "^2.79.0",
    "sax": "^1.2.1",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "@types/fs-extra": "^0.0.37",
    "@types/node": "^7.0.5",
    "@types/request": "^0.0.39",
    "@types/sax": "^0.0.28",
    "@types/tmp": "^0.0.32",
    "typescript": "^2.1.5"
  }
}
